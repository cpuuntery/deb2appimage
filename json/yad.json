{
    "buildinfo": [
    {
        "prerun": [
            "curl -sSL -o ~/.cache/deb2appimage/debs/yad.deb 'https://launchpad.net/~webupd8team/+archive/ubuntu/y-ppa-manager/+files/yad_0.39.0-1~webupd8~trusty0_amd64.deb'"
        ],
        "name": "yad",
        "version": "0.39",
        "deps": "libcairo2,libpango-1.0-0,libpangocairo-1.0-0,libsoup2.4-1,libwebkitgtk-3.0-0,libpng12-0,libjavascriptcoregtk-3.0-0,libgeoclue0,libicu52,libwebp5,libpangoft2-1.0-0",
        "repoarch": "amd64",
        "distrorepo": "Ubuntu",
        "repoversion": "trusty",
        "binarypath": "/usr/bin/yad",
        "desktoppath": null,
        "iconpath": "/usr/share/icons/hicolor/128x128/apps/yad.png",
        "usewrapper": "false",
        "postrun": [
            null
        ]
    }
    ],
    "apprunconf": [
    {
        "setpath": "true",
        "setlibpath": "true",
        "setpythonpath": "false",
        "setpythonhome": "false",
        "setpythondontwritebytecode": "false",
        "setxdgdatadirs": "false",
        "setperllib": "false",
        "setgsettingsschemadir": "false",
        "setqtpluginpath": "false",
        "exec": "/usr/bin/yad"
    }
    ],
    "authors": [
    {
        "type": "Author",
        "author": "YAD",
        "url": "YAD"
    },
    {
        "type": "AppImage Maintainer",
        "author": "simonizor",
        "url": "http://www.simonizor.net"
    }
    ]
}
