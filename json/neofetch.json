{
    "buildinfo": [
    {
        "prerun": [
            "git clone https://github.com/dylanaraps/neofetch.git ~/.cache/deb2appimage/neofetch",
            "mkdir -p ~/.cache/deb2appimage/AppDir/usr/bin",
            "cp -r ~/.cache/deb2appimage/neofetch/* ~/.cache/deb2appimage/AppDir/usr/bin/"
        ],
        "name": "neofetch",
        "version": "git",
        "deps": null,
        "repoarch": null,
        "distrorepo": null,
        "repoversion": null,
        "binarypath": "/usr/bin/neofetch",
        "desktoppath": null,
        "iconpath": null,
        "usewrapper": "false",
        "postrun": [
            null
        ]
    }
    ],
    "apprunconf": [
    {
        "setpath": "false",
        "setlibpath": "false",
        "setpythonpath": "false",
        "setpythonhome": "false",
        "setpythondontwritebytecode": "false",
        "setxdgdatadirs": "false",
        "setperllib": "false",
        "setgsettingsschemadir": "false",
        "setqtpluginpath": "false",
        "exec": "/usr/bin/neofetch"
    }
    ],
    "authors": [
    {
        "type": "Author",
        "author": "dylanaraps",
        "url": "https://github.com/dylanaraps/neofetch"
    },
    {
        "type": "AppImage Maintainer",
        "author": "simonizor",
        "url": "https://github.com/simoniz0r"
    }
    ]
}
